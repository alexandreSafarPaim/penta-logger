{
    "name": "alexandresafarpaim/penta-logger",
    "version": "0.2.7",
    "description": "Real-time log streaming dashboard for Laravel. Monitor requests, errors, external APIs, jobs, and scheduled tasks with zero configuration.",
    "keywords": ["laravel", "logging", "debugging", "developer-tools", "real-time", "dashboard", "jobs", "queue", "scheduler"],
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/alexandreSafarPaim/penta-logger",
    "authors": [
        {
            "name": "Alexandre Safari Paim",
            "homepage": "https://github.com/alexandreSafarPaim"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.0|^9.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.23|^7.0",
        "phpunit/phpunit": "^9.5|^10.0"
    },
    "autoload": {
        "psr-4": {
            "PentaLogger\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PentaLogger\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "PentaLogger\\PentaLoggerServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
